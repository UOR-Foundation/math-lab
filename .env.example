# Environment Configuration
# Copy this file to .env.local for local development

# Application Environment
# Can be 'development', 'staging', or 'production'
VITE_APP_ENV=development

# Base API URL
# Used for backend API calls if applicable
VITE_API_BASE_URL=https://api.example.com

# Analytics ID (if using analytics)
# VITE_ANALYTICS_ID=UA-XXXXX-X

# Feature Flags
# VITE_FEATURE_ADVANCED_VISUALIZATION=true
# VITE_FEATURE_CLOUD_SYNC=false